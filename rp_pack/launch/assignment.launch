    <arg name="second_robot" default="false" />
    <arg name="fake_localisation" default= "false"/>
    <arg name="map_server" default= "true"/>
  </include>

   <!-- Localisation  -->
  <include file="$(find uol_cmp9767m_tutorial)/launch/amcl.launch"/>

  <!-- move_base -->
